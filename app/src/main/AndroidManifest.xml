<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="id.fahmi.abkberbakat"
    android:screenOrientation="landscape">

    <uses-permission android:name="android.permission.INTERNET" />

    <permission android:name="android.permission.MEDIA_CONTENT_CONTROL" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:screenOrientation="landscape"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar">
        <service android:name=".AudioBackground" />

        <activity
            android:name=".MainActivity"
            android:screenOrientation="landscape">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".KataBendaSayuranActivity"
            android:screenOrientation="landscape" />
        <activity
            android:name=".BermainActivity"
            android:screenOrientation="landscape" />
        <activity
            android:name=".MiniGame2Activity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/title_activity_mini_game2"
            android:screenOrientation="landscape"
            android:theme="@style/FullscreenTheme" />
        <activity
            android:name=".KategoriBelajarActivity"
            android:screenOrientation="landscape" />
        <activity
            android:name=".CobaActivity"
            android:screenOrientation="landscape" />
        <activity
            android:name=".KosakataActivity"
            android:screenOrientation="landscape" />
        <activity
            android:name=".GameTebakGambarActivity"
            android:screenOrientation="landscape" />
        <activity
            android:name=".SettingsActivity"
            android:label="@string/title_activity_settings"
            android:screenOrientation="landscape" />
        <activity
            android:name=".GameAritmatikaActivity"
            android:screenOrientation="landscape" />
        <activity
            android:name=".KategoriBermainActivity"
            android:screenOrientation="landscape" />
        <activity
            android:name=".KeteraanganKosakataActivity"
            android:screenOrientation="landscape" />
        <activity
            android:name=".TentangActivity"
            android:screenOrientation="landscape" />
        <activity
            android:name=".GameTebakJumlahObjekActivity"
            android:screenOrientation="landscape" />
        <activity
            android:name=".TebakSuaraActivity"
            android:screenOrientation="landscape" />
        <activity
            android:name=".GameTebakSuaraActivity"
            android:screenOrientation="landscape"></activity>
        <activity android:name=".GameTebakAngkaActivity"></activity>
    </application>

</manifest>